{
  "name": "taro-spe-ui",
  "version": "0.0.3",
  "browser": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "description": "基于Taro的一个UI组件库，简单易用",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist"
  ],
  "source": "src/index.ts",
  "scripts": {
    "dev": "yarn run dev:lib",
    "dev:lib": "tsc --project ./tsconfig.build.json --watch --incremental",
    "build": "yarn run build:rollup && yarn run build:lib",
    "build:lib": "tsc --project ./tsconfig.build.json --declaration --declarationDir dist/types",
    "build:rollup": "rollup --config config/rollup.config.js",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test": "jest",
    "prettier": "prettier --write \"src/**/*.ts*\""
  },
  "keywords": [
    "taro",
    "react"
  ],
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.21.5",
    "@tarojs/components": "3.6.16",
    "@tarojs/helper": "3.6.16",
    "@tarojs/plugin-framework-react": "3.6.16",
    "@tarojs/plugin-platform-alipay": "3.6.16",
    "@tarojs/plugin-platform-h5": "3.6.16",
    "@tarojs/plugin-platform-jd": "3.6.16",
    "@tarojs/plugin-platform-qq": "3.6.16",
    "@tarojs/plugin-platform-swan": "3.6.16",
    "@tarojs/plugin-platform-tt": "3.6.16",
    "@tarojs/plugin-platform-weapp": "3.6.16",
    "@tarojs/react": "3.6.16",
    "@tarojs/runtime": "3.6.16",
    "@tarojs/shared": "3.6.16",
    "@tarojs/taro": "3.6.16",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@tarojs/cli": "3.6.16",
    "@tarojs/taro-loader": "3.6.16",
    "@tarojs/test-utils-react": "^0.1.1",
    "@tarojs/webpack5-runner": "3.6.16",
    "@types/jest": "^29.3.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "babel-preset-taro": "3.6.16",
    "eslint": "^8.12.0",
    "eslint-config-taro": "3.6.16",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.18",
    "prettier": "^3.0.3",
    "react-refresh": "^0.11.0",
    "rollup": "2.66",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-visualizer": "^4.0.4",
    "stylelint": "^14.4.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.0",
    "webpack": "5.78.0"
  }
}
